<p id="notice"><%= notice %></p>

<h1>Phone Session Types</h1>

<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="2"></th>
    </tr>
  </thead>

  <tbody>
    <% @phone_session_types.each do |phone_session_type| %>
      <tr>
        <td><%= phone_session_type.name %></td>
        <td class="table-actions">
          <div class="btn-group" role="group">  
            <%= link_to 'Edit', edit_phone_session_type_path(phone_session_type), :class => "btn btn-default" %>   
            <%= link_to 'Destroy', phone_session_type, :class => "btn btn-default", method: :delete, data: { confirm: 'Are you sure?' } %>
          </div>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Create a Phone Session Type', new_phone_session_type_path, :class => "btn btn-native" %>

